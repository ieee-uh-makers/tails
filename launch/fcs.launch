<launch>
  <node pkg="tails" type="fcs_node.py" name="fcs" output="screen">
    <param name="~armed" value="false"/>
    <param name="~gpio_ail" value="18"/>
    <param name="~gpio_ele" value="23"/>
    <param name="~gpio_thr" value="24"/>
    <param name="~gpio_rud" value="22"/>

    <param name="~land_ele" value="1500"/>
    <param name="~land_ail" value="1500"/>
    <param name="~land_thr" value="1200"/>
    <param name="~land_rud" value="1500"/>

    <param name="~min_ail" value="1000"/>
    <param name="~min_ele" value="1000"/>
    <param name="~min_thr" value="1000"/>
    <param name="~min_rud" value="1000"/>

    <param name="~max_ail" value="2000"/>
    <param name="~max_ele" value="2000"/>
    <param name="~max_thr" value="1500"/>
    <param name="~max_rud" value="2000"/>


    <param name="~pwm_freq" value="50"/>
    <param name="~watchdog_timeout" value="10"/>
  </node>
</launch>
